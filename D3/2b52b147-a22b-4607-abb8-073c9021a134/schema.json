                    {
                      "label": {
                        "en": "Input",
                        "fr": "Input"
                      },
                      "value": "input"
                    },
                    {
                      "label": {
                        "en": "Text Area",
                        "fr": "Zone de texte"
                      },
                      "value": "textarea"
                    },
                    {
                      "label": {
                        "en": "Select",
                        "fr": "Sélect"
                      },
                      "value": "select"
                    },
                    {
                      "label": {
                        "en": "Radio Group",
